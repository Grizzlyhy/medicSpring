<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zhou.code.mapper.BrowsingRecordMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.zhou.code.bean.BrowsingRecord">
        <id column="id" property="id" />
        <result column="user_id" property="userId" />
        <result column="ip" property="ip" />
        <result column="browse_module" property="browseModule" />
        <result column="operation_action" property="operationAction" />
        <result column="page_url" property="pageUrl" />
        <result column="operation_result" property="operationResult" />
        <result column="operation_time" property="operationTime" />
        <result column="request_code" property="requestCode" />
        <result column="remarks" property="remarks" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, user_id, ip, browse_module, operation_action, page_url, operation_result, operation_time, request_code, remarks
    </sql>

</mapper>
