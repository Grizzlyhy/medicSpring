<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zhou.code.mapper.CaseImageMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.zhou.code.bean.CaseImage">
        <result column="case_id" property="caseId" />
        <result column="case_name" property="caseName" />
        <result column="original_image_url" property="originalImageUrl" />
        <result column="result_image_url" property="resultImageUrl" />
        <result column="algorithm_id" property="algorithmId" />
        <result column="model_id" property="modelId" />
        <result column="user_id" property="userId" />
        <result column="create_time" property="createTime" />
        <result column="update_time" property="updateTime" />
        <result column="deleted" property="deleted" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        case_id, case_name, original_image_url, result_image_url, algorithm_id, model_id, user_id, create_time, update_time, deleted
    </sql>

</mapper>
